def es_primo(num):
	if  num<2:
		return False
	contador=0
	divisor =1
	while(divisor <= num):
		if (num%divisor==0):
			contador=contador+1
		divisor+=1
	if(contador==2):
		return True
	else:
		return False

def BubbleSort(V,n):
	for i in range(n):
		if es_primo(V[i]):
			for j in range (i,n):
				if es_primo(V[j]):
					if V[i] > V[j]:
						V[i], V[j]= V[j], V[i]
